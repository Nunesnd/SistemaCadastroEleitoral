@model SistemaCadastroEleitoral.Models.Observacao.ObservacaoModel

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>ObservacaoModel</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Conteudo" class="control-label"></label>
                <input asp-for="Conteudo" class="form-control" />
                <span asp-validation-for="Conteudo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DataObservacao" class="control-label" hidden></label>
                <input type="date" asp-for="DataObservacao" class="form-control" hidden />
            </div>
            <div class="form-group">
                @Html.HiddenFor(m => m.cadastroId)
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

